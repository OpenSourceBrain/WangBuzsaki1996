soma.L = 10./PI
soma.cm = 1
soma.Ra = 100
soma insert pas
soma.g_pas = 0.0001
soma.e_pas = -65
soma insert naf
soma.gmax_naf = 0.035
soma insert kdr
soma.gmax_kdr = 0.009
