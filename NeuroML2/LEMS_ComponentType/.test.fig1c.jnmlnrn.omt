target: LEMS_Fig1_C.xml
engine: jNeuroML_NEURON
mep: .test.fig1c.mep
experiments:
  c1:
    observables:
      spike times:
        file: 
          path: fig1c.dat
          columns: [0,1]
          scaling: [1000, 1000]
        spike detection: 
          method: threshold
          threshold: 0
        tolerance: 0
